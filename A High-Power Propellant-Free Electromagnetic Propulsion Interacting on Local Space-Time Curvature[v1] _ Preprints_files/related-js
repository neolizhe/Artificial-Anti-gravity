if(typeof jQuery=='undefined'){var script=document.createElement('script');script.type="text/javascript";script.src="https://code.jquery.com/jquery-3.4.1.min.js";document.getElementsByTagName('head')[0].appendChild(script);script.onload=loadWidget();script.onreadystatechange=function(){if(this.readyState=='complete'){loadWidget();}}}else{loadWidget();}
function loadWidget(){window.onload=function(){var holder=$('#scilit-related-articles-widget-holder');if(holder.length===0){return false;}
holder.hide();var data=holder.data();var doi=typeof data['doi']!='undefined'?data['doi']:'';var cacheKey=typeof data['cachekey']!='undefined'?data['cachekey']:'';var mode=typeof data['mode']!='undefined'?data['mode']:'';var fromMyPlatform=typeof data['frommyplatform']!='undefined'?1:0;var maxItems=typeof data['maxitems']!='undefined'?data['maxitems']:'';var keywords=typeof data['keywords']!='undefined'?data['keywords']:'';var dataSearch=typeof data['data-search']!='undefined'?1:0;var url='https://www.scilit.net/article/related-frame?doi='+encodeURI(doi);url+='&cache-key='+encodeURI(cacheKey);url+='&mode='+mode;url+='&from-my-platform='+fromMyPlatform;url+='&max-items='+maxItems;url+='&data-search='+dataSearch;url+='&keyword[]='+encodeURI(keywords.split(' | ').join('&keyword[]='));$.get(url,function(response){holder.html(response).show();$.getScript("https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.5/MathJax.js?config=default",function(){MathJax.Hub.Config({tex2jax:{inlineMath:[['$','$'],['\\(','\\)']]},"HTML-CSS":{styles:{".MathJax_Display":{display:"inline !important"}}},processClass:"sraw-title"});MathJax.Hub.Queue(["Typeset",MathJax.Hub]);});});};}